package model;

import javax.swing.ImageIcon;

public class OModel extends PieceModel {
	private ImageIcon x;
	
	public OModel() {
		this.setX(new ImageIcon(getClass().getResource("../resources/cruz.jpg")));
	}

	public ImageIcon getX() {
		return x;
	}

	public void setX(ImageIcon x) {
		this.x = x;
	}
}
