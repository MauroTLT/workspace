package FireEmblemGraphic;

public class TableroModel {
	
	private Casilla[][] tablero;
	
	public TableroModel() {
		this.tablero = new Casilla[][]
				{
			{new Planicie(p,0,0), new Planicie(p,0,1), new Planicie(p,0,2), new Planicie(p,0,3), new Planicie(p,0,4), new Planicie(p,0,5)},
			{new Planicie(p,1,0), new Planicie(p,1,1), new Planicie(p,1,2), new Casilla(m,1,3), new Arboleda(a,1,4), new Planicie(p,1,5)},
			{new Planicie(p,2,0), new Arboleda(a,2,1), new Casilla(m,2,2), new Casilla(m,2,3), new Planicie(p,2,4), new Planicie(p,2,5)},
			{new Planicie(p,3,0), new Planicie(p,3,1), new Casilla(m,3,2), new Casilla(m,3,3), new Planicie(p,3,4), new Planicie(p,3,5)},
			{new Planicie(p,4,0), new Planicie(p,4,1), new Casilla(m,4,2), new Casilla(m,4,3), new Arboleda(a,4,4), new Planicie(p,4,5)},
			{new Planicie(p,5,0), new Arboleda(a,5,1), new Casilla(m,5,2), new Casilla(m,5,3), new Planicie(p,5,4), new Planicie(p,5,5)},
			{new Planicie(p,6,0), new Planicie(p,6,1), new Casilla(m,6,2), new Arboleda(a,6,3), new Planicie(p,6,4), new Planicie(p,6,5)},
			{new Planicie(p,7,0), new Planicie(p,7,1), new Planicie(p,7,2), new Planicie(p,7,3), new Planicie(p,7,4), new Arboleda(a,7,5)}
		};
		darFormato();
	}

	private void darFormato() {
		
	}

	public Casilla[][] getTablero() {
		return tablero;
	}

	public void setTablero(Casilla[][] tablero) {
		this.tablero = tablero;
	}

}
