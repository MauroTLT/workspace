package FireEmblemGraphic;

public class Main {
	static Jugador j1 = new Jugador("Mauro");
	static Jugador j2 = new Jugador("Topo");
	
	public static void main(String[] args) {
		Main programa = new Main();
		programa.inici();
	}
	
	private void inici() {		
		TableroController tablero = new TableroController();
		tablero.getVentana().getTitulo().setText("Turno de : " + j1.getNombre());
		
	}

}
