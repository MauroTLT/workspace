package model;

import javax.swing.ImageIcon;

public class XModel extends PieceModel {
	private ImageIcon x;
	
	public XModel() {
		this.x = new ImageIcon(getClass().getResource("../resources/cruz.jpg"));
	}

	public ImageIcon getX() {
		return x;
	}

	public void setX(ImageIcon x) {
		this.x = x;
	}
}
