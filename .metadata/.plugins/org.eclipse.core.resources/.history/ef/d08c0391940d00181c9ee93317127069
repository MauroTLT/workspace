package ejemplo;

import javax.swing.*;

public class Main extends JFrame {
	public static void main(String[] args) {
		new Main();
	}
	// el constuctor
	public Main() {
		setTitle("Hola!!!");
		// Pone un título a la ventana
		setSize(800, 600);
		// método heredado que le da un tamaño a la ventana
		setVisible(true);
	}
}