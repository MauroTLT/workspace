package Model;

public class Combate extends Evento{
	
	private Personaje protagonista, enemigo;

	public Combate(Personaje protagonista, Personaje enemigo) {
		this.protagonista = protagonista;
		this.enemigo = enemigo;
	}

	public Personaje getProtagonista() {
		return protagonista;
	}

	public void setProtagonista(Personaje protagonista) {
		this.protagonista = protagonista;
	}

	public Personaje getEnemigo() {
		return enemigo;
	}

	public void setEnemigo(Personaje enemigo) {
		this.enemigo = enemigo;
	}

}
