package unidad8.practica5;

@SuppressWarnings("serial")
public class Excepciones extends Exception{
	private int valor;
	
	public String toString(int valor){
		String error = "";
		switch(valor) {
			case 1:
				error = "Dato no encontrado";
				break;
			case 2:
				error = "Lista vacia";
				break;
			case 3:
				
				break;
		}
		return "Error del tipo: " + error;
	}

}
