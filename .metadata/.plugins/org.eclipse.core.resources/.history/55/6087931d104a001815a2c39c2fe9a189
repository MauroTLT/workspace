package Model;

import Controller.BatallaController;
import View.BatallaView;

public class Combate extends Evento {
	
	//private BatallaView batalla;
	private static Combate combateSingleton = new Combate();
	
	public static Combate getInstance() {
		return combateSingleton;
	}
	
	public void combate(Casilla casilla) {
		System.out.println(casilla.getEdificio().getRuta());
		System.out.println(casilla.getPersonajes()[0].getRuta());
		System.out.println(casilla.getPersonajes()[1].getRuta());
		new BatallaController(casilla);
	}
}
