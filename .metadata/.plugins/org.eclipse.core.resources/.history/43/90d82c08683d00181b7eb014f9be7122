package FireEmblemGraphic;

public class TableroModel {
	
	private Casilla[][] tablero;
	
	public TableroModel() {
		this.tablero = new Casilla[][] {
			{new Planicie(), new Planicie(), new Planicie(), new Planicie(), new Planicie(), new Planicie()},
			{new Planicie(), new Planicie(), new Planicie(), new Mountain(), new Arboleda(), new Planicie()},
			{new Planicie(), new Arboleda(), new Mountain(), new Mountain(), new Planicie(), new Planicie()},
			{new Planicie(), new Planicie(), new Mountain(), new Mountain(), new Planicie(), new Planicie()},
			{new Planicie(), new Planicie(), new Mountain(), new Mountain(), new Arboleda(), new Planicie()},
			{new Planicie(), new Arboleda(), new Mountain(), new Mountain(), new Planicie(), new Planicie()},
			{new Planicie(), new Planicie(), new Mountain(), new Arboleda(), new Planicie(), new Planicie()},
			{new Planicie(), new Planicie(), new Planicie(), new Planicie(), new Planicie(), new Arboleda()}};
		darFormato();
	}

	private void darFormato() {
		
	}

	public Casilla[][] getTablero() {
		return tablero;
	}

	public void setTablero(Casilla[][] tablero) {
		this.tablero = tablero;
	}

}
